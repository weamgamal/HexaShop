@model List<Orders>

@{
    ViewData["Title"] = "showOrdersArrived";
}

<h1>showOrdersArrived</h1>

<div>
    <h4>OrdersArrived</h4>
    <hr />
    @foreach(var model in Model){
   


        <div class="main-banner" id="top" style="padding-top:100px">
            <div class="row row-cols-1 row-cols-md-3 g-4" style="padding:15px">
                @foreach (var item in Model)
                    {
                    <div class="col" style="padding:15px">       
                        <div class="card h-100">
                        
                            <div class="card-body">
                                <h5 class="card-title">@item.Customer</h5>
                                    @*price*@
                                    <span> @item.OrderStatus.Name LE</span>
                                    <span> @item.Customer.UserName</span>
                                    @*star*@
                                    <br/>
                            </div>
                        </div>
                    </div>
                }

            </div>
        </div>
    }
</div>

<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>
